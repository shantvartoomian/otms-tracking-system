<% if (result) { %>
  <div class="  text-center body-container" id="OH" ><button class="btn btn-dark fa fa-moon float-left" onclick="darkLight()"></button>
    <h3 class="text-center body-container">Results</h3><hr>
    
      <div class="container body-container"  style="max-width: 100%;">
        <article class="card body-container " >
            <!-- <header class="card-header body-container"> My Orders / Tracking </header> -->
            <div class="card-body body-container" >
              <span id="printableArea" ><span id="printableArea">
                <article class="card body-container">
                    <div class="card-body row body-container">
                      <% if (result[0].PNo) { %>
                        <div class="col"> <strong>PNo:</strong> <br><%=result[0].PNo%> </div>
                        <% } else { %>
                          <div class="col"> <strong>PNo:</strong> <br> - </div>
                      <% } %>
                        
                      <% if (result[0].FileNo) { %>
                        <div class="col"> <strong>FileNo:</strong> <br><%=result[0].FileNo %> </div>
                        <% } else { %>
                          <div class="col"> <strong>FileNo:</strong> <br> - </div>
                      <% } %>

                      <% if (result[0].HBL) { %>
                        <div class="col"> <strong>HBL:</strong> <br> <span > <%=result[0].HBL %></span>  </div>
                        <% } else { %>
                          <div class="col"> <strong>HBL:</strong> <br> <span > - </span>  </div>
                      <% } %>

                      <% if (result[0].BLIssueDateH) { %>
                        <div class="col"> <strong>BLIssueDateH:</strong> <br> <%=result[0].BLIssueDateH %> </div>
                        <% } else { %>
                          <div class="col"> <strong>BLIssueDateH:</strong> <br> - </div>
                      <% } %>
                        
                    </div>

                    <div class="card-body row">

                      <% if (result[0].ETA) { %>
                        <div class="col"> <strong>ETA:</strong> <br> <%=result[0].ETA %></div>
                        <% } else { %>
                          <div class="col"> <strong>ETA:</strong> <br> - %></div>
                      <% } %>

                      <% if (result[0].ArrivalDate) { %>
                        <div class="col"> <strong>ArrivalDate:</strong> <br> <%=result[0].ArrivalDate %> </div>
                        <% } else { %>
                          <div class="col"> <strong>ArrivalDate:</strong> <br> - </div>
                      <% } %>

                      <% if (result[0].DORcvdDate) { %>
                        <div class="col"> <strong>DORcvdDate:</strong> <br> <%=result[0].DORcvdDate %> </div> 
                        <% } else { %>
                          <div class="col"> <strong>DORcvdDate:</strong> <br> - </div>
                      <% } %>
                      
                      <% if (result[0].DOSendDate) { %>
                        <div class="col"> <strong>DOSendDate:</strong> <br> <%=result[0].DOSendDate %> </div>
                        <% } else { %>
                          <div class="col"> <strong>DOSendDate:</strong> <br> - </div>
                      <% } %>
                      
                  </div>


                  
                  
                
              <div class="card-body row">

                    <% if (result[0].POL) { %>
                      <div class="col-sm-3"> <strong>POL:</strong> <br><%=result[0].POL %> </div>
                      <% } else { %>
                        <div class="col-sm-3"> <strong>POL:</strong> <br> - </div>
                    <% } %>
                
                    <% if (result[0].POT) { %>
                      <div class="col-sm-3"> <strong>POT:</strong> <br> <%=result[0].POT %> </div>
                      <% } else { %>
                       <div class="col-sm-3"> <strong>POT:</strong> <br> - </div>
                    <% } %>
                
                    <% if (result[0].POD) { %>
                      <div class="col-sm-3"> <strong>POD:</strong> <br> <%=result[0].POD %> </div>
                      <% } else { %>
                        <div class="col-sm-3"> <strong>POD:</strong> <br> - </div>
                    <% } %>
   
            </div>

          <!-- tables for multiple container -->

<hr>
            <div style="height: 250px !important;
            overflow: scroll; max-width: 100%; ">
            
          <div class="list-group"  >
                   

              
              <div class="table-responsive ">
                <table class="table  table-striped table-default table-sm "  width="100%" id="myTable" style="overflow-y: scroll;">
                  <% if (result[0].TransportationMode == 1 || result[0].TransportationMode == 5) { %>
                    <thead style="background-color: rgb(173, 171, 171);">
                      <tr>
                        <th class="th-sm" onclick="sortTable(0)">CNTR</th>
                        <th class="th-sm" onclick="sortTable(1)">GateOut</th>
                        <th class="th-sm" onclick="sortTable(2)">MTRTN</th>
                        
      
                      </tr>
                    </thead>
                    <tbody>
                      <% result.forEach(item => { %>  
                      <tr>
                        <td><%=item.Sea_CNTR%></td>
                        <td><%=item.Sea_GateOut%></td>
                        <td><%=item.Sea_MTRTN%></td>
                        
      
                      </tr>
                      <% }) %>
                    </tbody>
                    
                  <% } %>
                  <% if (result[0].TransportationMode == 3) { %>
                    <thead style="background-color: rgb(173, 171, 171);">
                      <tr>
                        <th onclick="sortTable(0)">OverLand_TruckNo</th>
                        <th onclick="sortTable(1)">OverLand_Dispatch</th>
                        <th onclick="sortTable(2)">OverLand_BorderCrossing</th>
                        <th onclick="sortTable(3)">OverLand_Discharge</th>  
      
                      </tr>
                    </thead>
                    <tbody>
                      <% result.forEach(item => { %>  
                      <tr>
                       
                        <td><%=item.OverLand_TruckNo%></td>
                        <td><%=item.Overland_Dispatch%></td>
                        <td><%=item.Overland_BorderCrossing%></td>
                        <td><%=item.Overland_Discharge%></td>
      
                      </tr>
                      <% }) %>
                    </tbody>
                  <% } %>
                  

                  

                </table>

                

                
              </div>
              <!-- ------------------------------------------------------------ -->

              <!-- ------------------------------------------------------------ -->
           
          </div>
        </div>
         



          <!-- end of tables for multiple container -->
  </article>



          <% if (result[0].TransportationMode == 1 || result[0].TransportationMode == 5) { %>
