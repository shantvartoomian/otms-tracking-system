              <div class="step  active "  > <span class="icon"> <i class="fa fa-truck"></i> </span> <span class="text darkColorWhite">Border Crossed</span> </div>
              <div class="step  active "  > <span class="icon"> <i class="fa fa-check"></i> </span> <span class="text darkColorWhite">Border Discharged</span> </div>


          </div><br><br></span>
          <hr>

            <% } %>

